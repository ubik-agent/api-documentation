{
  "$schema": "https://mintlify.com/docs.json",
  "name": "UBIK Public API",
  "theme": "aspen",
  "logo": {
    "light": "/logo/logo-light.png",
    "dark": "/logo/logo-dark.png"
  },
  "favicon": "/logo/favicon.png",
  "colors": {
    "primary": "#06b6d4",
    "light": "#22d3ee",
    "dark": "#0891b2"
  },
  "background": {
    "color": {
      "light": "#FFFFFF",
      "dark": "#0F172A"
    },
    "decoration": "grid"
  },
  "fonts": {
    "family": "Inter"
  },
  "icons": {
    "library": "lucide"
  },
  "search": {
    "prompt": "Search the UBIK Agents API..."
  },
  "styling": {
    "eyebrows": "breadcrumbs"
  },
  "navigation": {
    "languages": [
      {
        "language": "en",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "/en/index",
              "/en/quickstart",
              "/en/development"
            ]
          },
          {
            "group": "API Reference",
            "pages": [
              "/en/api-reference/introduction",
              {
                "group": "Endpoints",
                "openapi": "api-reference/openapi.en.json"
              }
            ]
          }
        ]
      },
      {
        "language": "fr",
        "groups": [
          {
            "group": "Pour commencer",
            "pages": [
              "/fr/index",
              "/fr/demarrage-rapide",
              "/fr/guide-developpement"
            ]
          },
          {
            "group": "Référence API",
            "pages": [
              "/fr/api-reference/introduction",
              {
                "group": "Ressources API",
                "openapi": "api-reference/openapi.fr.json"
              }
            ]
          }
        ]
      }
    ]
  },
  "navbar": {
    "links": [
        {
            "label": "Go to App",
            "href": "https://app.ubik-agent.com"
        }
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/ubik-agent"
    }
  },
  "api": {
    "examples": {
      "languages": [
        "python",
        "javascript",
        "curl"
      ]
    },
    "params": {
      "expanded": "all"
    }
  },
  "contextual": {
    "options": [
      "copy"
    ]
  },
  "banner": {
    "content": "UBIK Agent will launch soon, [join the waitlist to get access!](https://ubik-agent.com/)"
  }
}